{
  "name": "ConsumIt",
  "description": "ConsumIt is a resource manager which estimates how long it will take to consume some link",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "storage",
    "webNavigation"
  ],
  "browser_action": {
      "default_title": "ConsumIt",
      "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery-2.1.3.js", "clickMenu.js"]
  },
  "manifest_version": 2
}
